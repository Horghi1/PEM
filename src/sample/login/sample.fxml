<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="512.0" prefWidth="729.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.login.LoginController">
    <children>
        <BorderPane prefHeight="512.0" prefWidth="669.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <center>
                <VBox alignment="CENTER" focusTraversable="true" prefHeight="298.0" prefWidth="375.0" BorderPane.alignment="CENTER">
                    <children>
                  <Label alignment="CENTER" contentDisplay="RIGHT" text="Welcome">
                     <font>
                        <Font name="Baskerville Old Face" size="20.0" />
                     </font>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <HBox alignment="CENTER" prefHeight="45.0" prefWidth="468.0">
                     <children>
                        <Label text="      Email:">
                           <font>
                              <Font size="13.0" />
                           </font>
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="emailTextField" alignment="CENTER" prefHeight="25.0" prefWidth="342.0">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin></TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="48.0" prefWidth="468.0">
                     <children>
                        <Label text="Password:">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                        <PasswordField fx:id="passwordTextField" alignment="CENTER" prefHeight="25.0" prefWidth="342.0">
                           <effect>
                              <DropShadow />
                           </effect>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin></PasswordField>
                     </children>
                  </HBox>
                        <Button fx:id="loginButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#pressLoginButton" text="Login">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin></Button>
                    </children>
               <effect>
                  <Glow />
               </effect>
                </VBox>
            </center>
         <top>
            <Label fx:id="logoLabel" alignment="CENTER" contentDisplay="CENTER" focusTraversable="false" text="Private Expense Manager" textAlignment="CENTER" textFill="#220101" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Baskerville Old Face" size="32.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </Label>
         </top>
         <bottom>
            <VBox prefHeight="86.0" prefWidth="468.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="errorLabel" alignment="CENTER" textFill="RED" />
                     </children>
                  </HBox>
                  <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="registerLabel" onMouseClicked="#pressRegisterLabel" text="If you don't have an account, click here..." textFill="#0195ff">
                           <font>
                              <Font size="13.0" />
                           </font>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                           <HBox.margin>
                              <Insets bottom="10.0" right="10.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </bottom>
         <left>
            <VBox alignment="CENTER" prefHeight="290.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets right="-20.0" />
               </BorderPane.margin>
            </VBox>
         </left>
         <right>
            <VBox prefHeight="290.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
         </right>
        </BorderPane>
    </children>
</AnchorPane>
