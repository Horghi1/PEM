<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="512.0" prefWidth="729.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.register.RegisterController">
   <children>
      <BorderPane layoutX="-1.0" layoutY="-2.0" prefHeight="512.0" prefWidth="669.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <top>
            <Label text="Registration Form" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Baskerville Old Face" size="32.0" />
               </font>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </Label>
         </top>
         <left>
            <VBox prefHeight="302.0" prefWidth="130.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="First Name:">
                     <VBox.margin>
                        <Insets left="60.0" top="50.0" />
                     </VBox.margin>
                     <font>
                        <Font size="13.0" />
                     </font></Label>
                  <Label id="lastNameFieldRegistration" text="Last Name:">
                     <VBox.margin>
                        <Insets left="60.0" top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font size="13.0" />
                     </font></Label>
                  <Label id="emailFieldRegistration" text="Email:">
                     <VBox.margin>
                        <Insets left="90.0" top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font size="13.0" />
                     </font></Label>
                  <Label id="passwordFieldRegistration" text="Password:">
                     <VBox.margin>
                        <Insets left="66.0" top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font size="13.0" />
                     </font></Label>
                  <Label id="confirmPassswordFieldRegistration" alignment="CENTER" text="Confirm Password:">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets left="18.0" top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font size="13.0" />
                     </font></Label>
               </children>
            </VBox>
         </left>
         <center>
            <VBox prefHeight="356.0" prefWidth="464.0" BorderPane.alignment="CENTER">
               <children>
                  <TextField fx:id="firstNameTextField">
                     <VBox.margin>
                        <Insets top="47.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="lastNameTextField">
                     <VBox.margin>
                        <Insets top="15.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="emailTextField">
                     <VBox.margin>
                        <Insets top="14.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="passwordTextField">
                     <VBox.margin>
                        <Insets top="14.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="confirmPasswordTextField">
                     <VBox.margin>
                        <Insets top="14.0" />
                     </VBox.margin>
                  </TextField>
               </children></VBox>
         </center>
         <bottom>
            <VBox alignment="CENTER" prefHeight="90.0" prefWidth="554.0" BorderPane.alignment="BOTTOM_CENTER">
               <children>
                  <Button id="submitFieldRegistration" fx:id="submitButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#pressSubmitButton" text="Submit">
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </Button>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="errorLabel" textFill="RED">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="loginLabel" onMouseClicked="#pressloginLabel" text="If you already have an account, click here...." textFill="#0195ff">
                           <font>
                              <Font size="13.0" />
                           </font>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                           <HBox.margin>
                              <Insets bottom="10.0" right="10.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </bottom>
         <right>
            <VBox prefHeight="302.0" prefWidth="130.0" BorderPane.alignment="CENTER" />
         </right></BorderPane>
   </children>
</AnchorPane>
